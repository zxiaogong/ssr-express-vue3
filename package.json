{
  "name": "ssr-express-vue3",
  "version": "1.1.0",
  "description": "基于vite插件搭建的vue3服务端渲染项目",
  "author": "zxiaogong",
  "scripts": {
    "dev": "nodemon --exec ts-node server",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr framework/entry/entry-server.ts",
    "build:app": "vite build --outDir dist/app --ssr server.ts",
    "build": "yarn run build:app && yarn run build:server && yarn run build:client",
    "app": "node dist/app/server.mjs"
  },
  "keywords": [],
  "license": "ISC",
  "imports": {},
  "_moduleAliases": {
    "@framework": "framework",
    "@server": "src/server",
    "@router": "src/router"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [],
    "verbose": true,
    "execMap": {},
    "watch": [
      "framework/server/*",
      "framework/entry/entry-client",
      "src/server/*",
      "src/router/serverRouter/*"
    ],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "ts"
  },
  "dependencies": {
    "ant-design-vue": "^3.3.0-beta.4",
    "axios": "^1.1.2",
    "express": "^4.18.1",
    "mysql": "^2.18.1",
    "uuid": "^9.0.0",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@vitejs/plugin-vue": "^3.2.0",
    "@vitejs/plugin-vue-jsx": "^2.1.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.3",
    "vite": "^3.2.4"
  }
}
