{
  "name": "ssr-express-vue3",
  "version": "1.0.1",
  "description": "基于vite插件搭建的vue3服务端渲染项目",
  "author": "zxiaogong",
  "scripts": {
    "dev": "nodemon --exec ts-node server.ts",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr framework/web/entry-server.ts"
  },
  "keywords": [],
  "license": "ISC",
  "imports": {},
  "_moduleAliases": {
    "@framework": "framework",
    "@src": "src"
  },
  "nodemonConfig": {
    "restartable": "rs",
    "ignore": [
      ".git",
      ".svn",
      "node_modules/**/node_modules",
      "framework/web/**",
      "src/router/webRouter/**",
      "src/web/**"
    ],
    "verbose": true,
    "execMap": {},
    "watch": [],
    "env": {
      "NODE_ENV": "development"
    },
    "ext": "ts"
  },
  "dependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.7.23",
    "@vitejs/plugin-vue": "^3.1.0",
    "@vitejs/plugin-vue-jsx": "^2.0.1",
    "axios": "^1.1.2",
    "express": "^4.18.1",
    "module-alias": "^2.2.2",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "vue": "^3.2.39",
    "vue-router": "^4.1.5"
  }
}