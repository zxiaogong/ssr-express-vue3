{
  "compilerOptions": {
    "jsx": "preserve",
    "target": "es2016",
    "module": "commonjs",
    "moduleResolution": "node",
    "lib": [
      "es2016",
      "dom"
    ],
    "outDir": "./dist",
    "esModuleInterop": true,
    "types": [
      "vite/client"
    ],
    "paths": {
      "@framework/*": [
        "./framework/*"
      ],
      "@src/*": [
        "./src/*"
      ],
    },
  },
  "exclude": [
    "node_modules",
    "vite/client"
  ],
}